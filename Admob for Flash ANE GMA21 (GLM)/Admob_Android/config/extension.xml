<extension xmlns="http://ns.adobe.com/air/extension/32.0">
    <id>com.glm.admob</id>
    <versionNumber>0.0.1</versionNumber>
    <platforms>
        <platform name="Android-ARM">
            <applicationDeployment>
                <nativeLibrary>admob21.3.0.jar</nativeLibrary>
                <initializer>com.glm.admob.Extension</initializer>
                <finalizer>com.glm.admob.Extension</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="Android-x86">
            <applicationDeployment>
                <nativeLibrary>admob21.3.0.jar</nativeLibrary>
                <initializer>com.glm.admob.Extension</initializer>
                <finalizer>com.glm.admob.Extension</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="Android-ARM64">
            <applicationDeployment>
                <nativeLibrary>admob21.3.0.jar</nativeLibrary>
                <initializer>com.glm.admob.Extension</initializer>
                <finalizer>com.glm.admob.Extension</finalizer>
            </applicationDeployment>
        </platform>
	<platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libadmob.a</nativeLibrary>
                <initializer>ExtInitializer</initializer>
                <finalizer>ExtFinalizer</finalizer>
            </applicationDeployment>
        </platform>	
        <platform name="default">
            <applicationDeployment/>
        </platform>
    </platforms>
</extension>
